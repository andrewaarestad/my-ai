{
  "name": "my-ai",
  "version": "0.1.0",
  "private": true,
  "description": "My AI - Personal data sync and search CLI",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.22.0",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,mdx,css,yml,yaml}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "pr-check": "pnpm format:check && turbo run lint type-check test build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate || echo 'Skipping Prisma generation (DATABASE_URL not set)'",
    "auth:google": "pnpm --filter @my-ai/cli auth:google",
    "sync:gmail": "pnpm --filter @my-ai/cli sync:gmail",
    "data:search": "pnpm --filter @my-ai/cli data:search"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^6.19.0",
    "tsx": "^4.19.0",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0"
  }
}
